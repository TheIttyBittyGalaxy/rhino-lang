struct Card {
    enum Suit { DIAMONDS , HEARTS , SPADES , CLUBS }
    Suit suit;
    int value;
}

fn main() {
    Card card;
    card.suit = SPADES;
    card.value = 1;

    Card.Suit trumps = DIAMONDS;
    
    > card.suit;
    > card.value;
    > card.suit == trumps;
}

// SUCCESS
// SPADES
// 1
// false